{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "strictBindCallApply": true,
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "types": ["vitest/importMeta"]
  },
  "include": ["src"]
}
